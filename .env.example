# Admin Authentication
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=your_secure_password_here

# Builder.io Configuration
# Get your API key from: https://builder.io/account/space
# Public API Key (safe to expose in client-side code)
VITE_BUILDER_PUBLIC_KEY=your_builder_public_key_here

# Cloudflare API Credentials (for Pages Functions)
# Get these from: https://dash.cloudflare.com/
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_STREAM_TOKEN=your_stream_api_token_here
CLOUDFLARE_IMAGES_TOKEN=your_images_api_token_here
